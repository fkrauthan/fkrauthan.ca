{
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 120,
  "semi": true,
  "bracketSameLine": true,
  "trailingComma": "es5",

  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^[^.]", "^[./].*(?<!\\.css)$", "^[./].*\\.(css)$"],
  "importOrderSeparation": true,
  "importOrderParserPlugins": ["jsx"]
}
